
<div class="booyah-box col-10 offset-1">
    <%= simple_form_for @address do |f| %>
     <%= f.error_notification %>
      <%= f.input :street %>

      <%= f.input :city %>

      <%= f.input :state %>

      <%= f.input :zip %>

      <%= f.button :submit, class: "btn-primary" %>
      <%= f.button :button, "Cancel", type: "reset", class: "btn-outline-secondary" %>
    <% end %>
</div>